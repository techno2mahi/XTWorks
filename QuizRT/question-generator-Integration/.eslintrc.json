{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "jquery": true
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "no-new": "off",
        "no-restricted-syntax": "off",
        "no-alert": "off",
        "no-unused-vars": "off",
        "one-var": "off",
        "no-tabs": "off",
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off",
        "func-names": "off",
        "no-use-before-define": "off",
        "guard-for-in": "off"
    }
}